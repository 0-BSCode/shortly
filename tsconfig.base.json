{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@auth-use-cases": ["libs/backend/use-cases/auth/src/index.ts"],
      "@dto": ["libs/dto/src/index.ts"],
      "@passport-auth-service": [
        "libs/backend/frameworks/auth-services/passport-auth-service/src/index.ts"
      ],
      "@services/auth-service": [
        "libs/backend/services/auth-service/src/index.ts"
      ],
      "@services/config-service": [
        "libs/backend/services/config-service/src/index.ts"
      ],
      "@services/data-service": [
        "libs/backend/services/data-service/src/index.ts"
      ],
      "@services/url-service": [
        "libs/backend/services/url-service/src/index.ts"
      ],
      "@smolurl-url-service": [
        "libs/backend/frameworks/url-services/smolurl-url-service/src/index.ts"
      ],
      "@typeorm-data-service": [
        "libs/backend/frameworks/data-services/typeorm-data-service/src/index.ts"
      ],
      "@url-use-cases": ["libs/backend/use-cases/url/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
